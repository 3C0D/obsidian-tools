{
	"name": "obsidian-my-tools",
	"version": "1.5.0",
	"description": "import, export, move...",
	"main": "main.js",
	"type": "module",
	"overrides": {
		"esbuild": "$esbuild"
	},
	"scripts": {
		"start": "npm install && npm run dev",
		"dev": "tsx scripts/esbuild.config.ts",
		"build": "tsc -noEmit -skipLibCheck && tsx scripts/esbuild.config.ts production",
		"real": "tsx scripts/esbuild.config.ts production -r",
		"acp": "tsx scripts/acp.ts",
		"bacp": "tsx scripts/acp.ts -b",
		"update-version": "tsx scripts/update-version.ts",
		"v": "tsx scripts/update-version.ts",
		"release": "tsx scripts/release.ts",
		"r": "tsx scripts/release.ts",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix"
	},
	"keywords": [],
	"author": "3C0D",
	"license": "MIT",
	"devDependencies": {
		"@types/eslint": "latest",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "latest",
		"@types/semver": "^7.5.8",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"builtin-modules": "3.3.0",
		"dedent": "^1.5.1",
		"dotenv": "^16.4.5",
		"esbuild": "latest",
		"eslint": "latest",
		"fs-extra": "^11.2.0",
		"obsidian": "latest",
		"obsidian-typings": "latest",
		"semver": "^7.6.0",
		"tslib": "2.4.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2",
		"@popperjs/core": "^2.11.8",
		"monkey-around": "^2.3.0"
	}
}